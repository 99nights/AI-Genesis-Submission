{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev:full",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/qdrant/(.*)",
      "destination": "/api/qdrant/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}

